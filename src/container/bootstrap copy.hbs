                <label class="form-check-label">Apple</label>
                <input name="food" type="checkbox" class="form-check-input">
            </div>
            <div class="form-check">
                <label class="form-check-label">Orange</label>
                <input name="food" type="checkbox" class="form-check-input">
            </div>
            <div class="form-check">
                <label class="form-check-label">Kiwi</label>
                <input name="food" type="checkbox" class="form-check-input">
            </div>
        </div>

        <div>
            <hr>
        </div>

        <div class="col-12">
            <div class="form-label">Select food</div>
            <div class="form-check">
                <label class="form-check-label">Apple</label>
                <input name="food" type="radio" class="form-check-input">
            </div>
            <div class="form-check">
                <label class="form-check-label">Orange</label>
                <input name="food" type="radio" class="form-check-input">
            </div>
            <div class="form-check">
                <label class="form-check-label">Kiwi</label>
                <input name="food" type="radio" class="form-check-input">
            </div>
        </div>

        <div>
            <hr>
        </div>

        <div class="col-12">
            <div class="form-label">Get bonus</div>
            <div class="form-check form-switch">
                <label class="form-check-label">Yes</label>
                <input name="food" type="checkbox" class="form-check-input">
            </div>
            <div class="form-check form-switch">
                <label class="form-check-label">No</label>
                <input name="food" type="checkbox" class="form-check-input">
            </div>
        </div>
