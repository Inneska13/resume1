<main>
    {{#with heading}}
    <div class="card">
        <div class="card-body">
            {{#with main}}
            <div class="card">
                <div class="card-body">

                    <h1 class="card-title">
                        {{title}}
                    </h1>
                    <p>{{text}}</p>

                    {{#with button_text}}
                    <div>
                        <button type="button" class="btn btn-primary">{{solid}}</button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-primary ">{{outline}}</button>
                    </div>
                    {{/with}}

                    {{/with}}
                </div>
            </div>
            <br>

            <div class="card">
                <div class="card-body">
                    {{#with second}}
                    <h2 class="card-title">{{title}}</h2>
                    <p>{{text}}</p>
                    {{#with button_text}}
                    <div>
                        <button type="button" class="btn btn-secondary">{{solid}}</button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-secondary ">{{outline}}</button>
                    </div>
                    {{/with}}
                    {{/with}}
                </div>
            </div>

            <br>

            <div class="card">
                <div class="card-body">
                    {{#with subblock}}
                    <h3 class="card-title">{{title}}</h3>
                    <p>{{text}}</p>
                    {{#with button_text}}
                    <div>
                        <button type="button" class="btn btn-dark">{{solid}}</button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-dark ">{{outline}}</button>
                    </div>
                    {{/with}}
                    {{/with}}
                </div>
            </div>

        </div>
    </div>
    {{/with}}

    <br>

    {{#with nested_display}}
    <div class="card">
        <div class="card-body">
            <div class="display-1">{{title}}</div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with children_display}}
                    <div class="display-4">{{title}}</div>
                    <p>{{text}}</p>
                    <button type="button" class="btn btn-outline-warning">{{button.text}}</button>
                    {{/with}}
                </div>
            </div>
            <br>
            <h6 class="card-title">{{header_text}}</h6>
            <br>
            <button type="button" class="btn btn-info">{{button.text}}</button>
        </div>
    </div>
    {{/with}}
    <br>

    {{#with paragraph}}
    <div class="card">
        <div class="card-body">
            <p class="text-uppercase">{{title}}</p>
            <p class="text-lowercase">{{text}}</p>
            <a href="{{button.url}}" class="btn btn-link">
                {{button.text}}
            </a>
            <br>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#each list_name}}
                    <ul>
                        <li>
                            <button class="btn btn-light text-primary"><a href="{{url}}"></a>
                                {{text}}
                            </button>
                        </li>
                    </ul>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>

    {{/with}}
</main>