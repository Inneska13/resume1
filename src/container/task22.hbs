{{#with page_header}}
<h1 class="text-center">{{title}}</h1>
<p class=" text-center text-uppercase">{{description}}</p>
{{/with}}
<main>
    {{#with section}}

    {{#with about_us}}
    <div class="card">
        <div class="card-body">
            {{#with title_list}}
            <div class="text-end display-6">{{main}}</div>
            <h2 class="text-end ">{{sub}}</h2>
            {{/with}}
            <br>
            {{#with button}}
            <button type="button" class="btn btn-outline-info">{{text}}</button>
            {{/with}}
        </div>
    </div>
    {{/with}}
    <br>

    {{#with advantage}}
    <div class="card">
        <div class="card-body">
            <div class="display-5 text-muted">{{title}}</div>
            <br>
            {{#with efficiency}}
            <div class="card">
                <div class="card-body">
                    <div class="display-4 text-muted">{{title}}</div>
                    <br>
                    <h6 class="card-title">{{sub_title}}</h6>
                    <br>
                    <p>{{description}}</p>
                    <br>
                    <button type="button" class="btn btn-outline-warning">{{button}}</button>

                </div>
            </div>
            {{/with}}
            <br>

            {{#with innovation}}
            <div class="card">
                <div class="card-body">
                    <div class="text-center text-muted display-4">{{title}}</div>
                    <br>
                    <h6 class=" text-center">{{sub_title}}</h6>
                    <br>
                    <p class="text-center text-muted">{{description}}</p>
                    <br>
                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                </div>
            </div>
            {{/with}}
            <br>
            {{#with collaboration}}
            <div class="card">
                <div class="card-body">
                    <div class="text-end text-muted display-4">{{title}}</div>
                    <br>
                    <h6 class=" text-end">{{sub_title}}</h6>
                    <br>
                    <p class="text-end text-muted">{{description}}</p>
                    <br>
                    <button type="button" class="btn btn-outline-warning">{{button}}</button>
                </div>
            </div>
            {{/with}}


        </div>
    </div>
    {{/with}}

    <br>
    {{#with agreement}}
    <div class="card">
        <div class="card-body">
            <div class=" display-5">{{title}}</div>
            <br>
            {{#with info}}
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">{{sub_title}}</h5>

                    {{#with detail}}
                    <div class="card">
                        <div class="card-body">
                            <p class="text-muted">{{text}}</p>
                            <br>
                            <button type="button" class="btn btn-success">{{button}}</button>

                        </div>
                    </div>
                    {{/with}}

                </div>
            </div>

            {{/with}}

        </div>
    </div>
    {{/with}}


    {{/with}}


</main>